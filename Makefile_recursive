# Makefile
# make
# make clean
# Issue make commands with the this outer Makefile.
# It then calls the inner Makefile
#

# myproject
# ├── hello1
# │   ├── f1.c
# │   ├── f1.h
# │   │── f2.c
# │   │── f2.h
# │   ├── main.c
# │   ├── Makefile  # inner Makefile
# │   └── print_log.h
# ├── hello2
# │   ├── f1.c
# │   ├── f1.h
# │   │── f2.c
# │   │── f2.h
# │   ├── main.c
# │   ├── Makefile  # inner Makefile
# │   └── print_log.h
# └── Makefile  # outer Makefile
#

all :
	$(MAKE) -C hello1
	$(MAKE) -C hello2

clean :
	$(MAKE) -C hello1 clean
	$(MAKE) -C hello2 clean

.PHONY : all clean
